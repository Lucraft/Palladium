{
  "schemaVersion": 1,
  "id": "palladium",
  "version": "${version}",
  "name": "Palladium",
  "description": "Core mod for all ThreeTAG mods.",
  "authors": [
    "Lucraft"
  ],
  "contact": {
    "homepage": "https://modrinth.com/project/threetag-palladium",
    "sources": "https://github.com/ThreeTAG/Palladium",
    "issues": "https://github.com/ThreeTAG/Palladium/issues"
  },
  "license": "All rights reserved",
  "icon": "palladium_icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "net.threetag.palladium.fabric.PalladiumFabric"
    ],
    "client": [
      "net.threetag.palladium.fabric.PalladiumFabricClient"
    ],
    "server": [
      "net.threetag.palladium.fabric.PalladiumFabricServer"
    ],
    "condensed_creative": [
      "net.threetag.palladium.compat.condensedcreative.CondensedCreativeCompat"
    ]
  },
  "mixins": [
    "palladium.mixins.json",
    "palladium-common.mixins.json"
  ],
  "depends": {
    "fabric": "*",
    "minecraft": ">=1.21",
    "palladiumcore": ">=2.2.0",
    "forgeconfigapiport": "*"
  },
  "custom": {
    "catalogue": {
      "banner": "palladium_banner.png"
    }
  }
}